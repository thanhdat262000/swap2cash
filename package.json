{
  "name": "nextjs-blog",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "dev:staging": "env-cmd -f .env.staging next dev",
    "build:dev": "env-cmd -f .env.development next build",
    "build:staging": "env-cmd -f .env.staging next build",
    "build:prod": "env-cmd -f .env.production next build",
    "start:dev": "env-cmd -f .env.development next start",
    "start:staging": "env-cmd -f .env.staging next start",
    "start:prod": "env-cmd -f .env.production next start",
    "lint": "eslint \"*.{ts,tsx,js,jsx}\" --color",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "prepare": "husky install",
    "prettier:check": "prettier --write .prettierrc.js --check \"*.{ts,js,tsx,jsx,scss,css,html}\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,js,tsx,jsx}": [
      "eslint --color",
      "npm run prettier:check",
      "git add ."
    ]
  },
  "dependencies": {
    "@ant-design/icons": "4.8.0",
    "@binance-chain/bsc-connector": "^1.0.0",
    "@ethersproject/contracts": "^5.4.1",
    "@ethersproject/providers": "^5.4.2",
    "@ethersproject/units": "^5.4.0",
    "@reduxjs/toolkit": "^1.8.0",
    "@types/uuid": "^9.0.1",
    "@walletconnect/browser": "^1.0.0",
    "@walletconnect/qrcode-modal": "^1.4.1",
    "@web3-react/abstract-connector": "^6.0.7",
    "@web3-react/core": "^6.1.9",
    "@web3-react/frame-connector": "^6.0.9",
    "@web3-react/injected-connector": "^6.0.7",
    "@web3-react/walletconnect-connector": "6.2.4",
    "@zeit/next-sass": "^1.0.1",
    "antd": "^5.4.5",
    "autoprefixer": "^10.4.14",
    "axios": "^0.21.1",
    "bignumber.js": "^9.1.1",
    "classnames": "^2.3.1",
    "ethers": "^5.4.3",
    "formik": "^2.2.9",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "next": "12.0.7",
    "next-compose-plugins": "^2.2.1",
    "next-i18next": "^8.1.3",
    "next-images": "1.8.4",
    "next-redux-wrapper": "7.0.5",
    "next-seo": "^4.24.0",
    "nookies": "^2.5.2",
    "postcss": "^8.4.22",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hot-toast": "^2.4.0",
    "react-icons": "^4.8.0",
    "react-number-format": "^5.1.4",
    "react-otp-input": "^2.3.0",
    "react-redux": "^7.2.6",
    "redux": "^4.1.2",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.3",
    "sass": "^1.49.9",
    "socket.io-client": "^4.4.1",
    "swiper": "^9.4.1",
    "swr": "^1.2.2",
    "tailwindcss": "^3.3.1",
    "uuid": "^9.0.0",
    "web3": "^1.3.6",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.179",
    "@types/node": "^17.0.21",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.3",
    "@types/socket.io-client": "^3.0.0",
    "@types/styled-components": "^5.1.9",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "eslint": "^8.12.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "12.1.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.4",
    "prettier": "^2.2.1",
    "typescript": "^4.6.2"
  }
}
